{
  "name": "megaplot",
  "version": "0.1.2",
  "description": "Engine for implementing high-performance, large-scale, interactive data visualizations.",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "rollup -c --bundleConfigAsCjs",
    "check-updates": "npm-check-updates",
    "debugging-demo": "webpack serve --open --entry ./src/demo/debugging-demo.ts",
    "generate": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' ts-node util/generate-code.ts",
    "http": "http-server --port 8081",
    "lint": "ESLINT_USE_FLAT_CONFIG=false eslint --config eslint.config.js src/",
    "prettier": "prettier 'src/**/*.ts' 'test/**/*.ts' 'util/**/*.ts' --write",
    "start": "npm run debugging-demo",
    "test": "karma start karma.conf.ts",
    "text-demo": "webpack serve --open --entry ./src/demo/text-demo.ts"
  },
  "author": "People + AI Research",
  "license": "Apache-2.0",
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "dependencies": {
    "regl": "2.1.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "24.0.1",
    "@rollup/plugin-node-resolve": "15.0.1",
    "@rollup/plugin-terser": "0.4.0",
    "@rollup/plugin-typescript": "11.0.0",
    "@types/d3": "7.4.0",
    "@types/dat.gui": "0.7.7",
    "@types/jasmine": "4.3.1",
    "@types/node": "18.15.3",
    "@types/stats": "0.16.30",
    "@typescript-eslint/eslint-plugin": "5.55.0",
    "@typescript-eslint/parser": "5.55.0",
    "clean-webpack-plugin": "4.0.0",
    "css-loader": "6.7.3",
    "d3": "7.8.2",
    "dat.gui": "0.7.9",
    "eslint": "8.36.0",
    "eslint-plugin-spellcheck": "0.0.20",
    "html-webpack-plugin": "5.5.0",
    "http-server": "14.1.1",
    "jasmine-core": "4.5.0",
    "karma": "6.4.1",
    "karma-chrome-launcher": "3.1.1",
    "karma-jasmine": "5.1.0",
    "karma-webpack": "5.0.0",
    "npm-check-updates": "16.7.12",
    "prettier": "2.8.4",
    "rollup": "3.19.1",
    "rollup-plugin-visualizer": "5.9.0",
    "stats.js": "0.17.0",
    "style-loader": "3.3.2",
    "ts-loader": "9.4.2",
    "ts-node": "10.9.1",
    "tslib": "2.5.0",
    "typescript": "4.9.5",
    "webpack": "5.76.1",
    "webpack-cli": "5.0.1",
    "webpack-dev-server": "4.12.0"
  }
}
